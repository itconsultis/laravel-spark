{
    "name": "itc/laravel-spark",
    "description": "Static site generator package for Laravel 5.1",
    "keywords": ["laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "phine/path": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "ITC\\Spark\\": "src/Spark/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"    
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
